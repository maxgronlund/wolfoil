<% @model.types.each do |type| %>
	<div class="grid_4 height_78 list-back">
		<div class="line_1"></div>
		<div class="grid_1 alpha margin_top_7">
			<%= link_to image_tag('../images/arrow-button.jpg',
									:mouseover=>'../images/arrow-button-over.jpg',
									:border=>0 ), 
									model_type_path( @model,type) %>
									
		</div>
		<div class="grid_3 omega">
			<div class="margin_top_7">
				<p><%=type.title%></p>
			</div>
		</div>
	</div>
<% end %>